{
  "name": "@repo/db",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src/generated"
  ],
  "scripts": {
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "db:generate": "prisma generate",
    "tsc:build": "tsc --build",
    "copy:generated": "node copy-generated.js",
    "build": "pnpm clean && pnpm db:generate && pnpm tsc:build && pnpm copy:generated"
  },
  "dependencies": {
    "@prisma/client": "6.6.0"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "cpy-cli": "^5.0.0",
    "fs-extra": "^11.2.0",
    "prisma": "6.6.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3"
  }
}
